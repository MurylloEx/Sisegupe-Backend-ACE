{
  "name": "default",
  "type": "sqlite",
  "database": "database/db.sqlite",
  "synchronize": true,
  "logging": true,
  "entities": [
    "src/models/**/*.ts"
  ],
  "migrations": [
    "src/migration/**/*.ts"
  ],
  "subscribers": [
    "src/subscriber/**/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/entity",
    "migrationsDir": "src/migration",
    "subscribersDir": "src/subscriber"
  }
}